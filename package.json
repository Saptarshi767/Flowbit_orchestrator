    "test:load:artillery": "artillery run tests/load/artillery-config.yml",
    "test:security": "python tests/security/security-test-suite.py",
    "test:chaos": "chaos run tests/chaos/chaos-experiments.yml",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:security",
    "test:report": "python tests/scripts/generate-test-report.py",
    "test-runner": "tsx tests/test-runner.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:test": "prisma migrate deploy",
    "db:seed:test": "prisma db seed",
    "db:reset:test": "prisma migrate reset --force",
    "db:studio": "prisma studio"
