    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest --watch",
    "test:integration": "vitest run tests/integration --run",
    "test:coverage": "vitest run --coverage",
